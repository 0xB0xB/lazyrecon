export HOMEUSER= # privilleged but non root user need e.g.: kali
export HOMEDIR= # user home dir e.g.: /home/kali
export STORAGEDIR=${HOMEDIR}/lazytargets # where output saved, e.g.: /home/kali/lazytargets

export LISTENSERVER=
export LISTENPORT=
export ATTACKER=${LISTENSERVER}:${LISTENPORT}
export ATTACKERURL=http://${LISTENSERVER}:${LISTENPORT}

export GITHUBTOKEN=
export DISCORDWEBHOOKURL=https://discord.com/api/webhooks/{webhook.id}/{webhook.token}

export GOPATH=$HOMEDIR/go
export PATH=$GOPATH/bin:$GOROOT/bin:$HOME/.local/bin:$PATH
export GO111MODULE=on
